{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules",
        "tslint-plugin-prettier",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": [true, {
            "singleQuote": true,
            "trailingComma": "all"
        }],
        "quotemark": [true, "single"],
        "interface-name": false,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
    }
}
